<!DOCTYPE html>
<html lang="zh-cn">

	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
		<title>{% block title %}首页{% endblock %}</title>
		
		{% block stylesheets %}
		  <link rel="stylesheet" href="{{ asset('public/pintuer2/css/pintuer.css') }}">
		  <link rel="stylesheet" href="{{ asset('public/static/css/base.css') }}">
		{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="{{ asset('favicon.ico') }}" rel="bookmark icon" />
	</head>

	<body>
	    {% block body %}
    	    {% block header %}
    	       {% include '@KitWeb/Common/header.html.twig' %}
    	    {% endblock %}
    	    
    	    {% block carousel %}
    	    {% endblock %}
    	    
    	    {% block nav %}
    	       {% include '@KitWeb/Common/nav.html.twig' %}
    	    {% endblock %}
    	    
    	    {% block content %}
    	    {% endblock %}
    	    
    	    {% block bottom %}
    	       {% include '@KitWeb/Common/bottom.html.twig' %}
    	    {% endblock %}
    	    {% block footer %}
    	       {% include '@KitWeb/Common/footer.html.twig' %}
    	    {% endblock %}
	    {% endblock %}
	
    	{% block javascripts %}
    	    <script src="{{ asset('public/pintuer2/js/jquery.js') }}"></script>
    		<script src="{{ asset('public/pintuer2/js/pintuer.js') }}"></script>
    		<script src="{{ asset('public/pintuer2/js/respond.min.js') }}"></script>
    	{% endblock %}
	</body>
</html>